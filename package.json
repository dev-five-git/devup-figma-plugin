{
	"name": "devup-figma-plugin",
	"version": "1.0.0",
	"description": "Devup Figma Plugin",
	"main": "code.js",
	"scripts": {
		"build": "bun build --target node src/code.ts --production --outfile=dist/code.js --format=cjs",
		"watch": "bun build --target node src/code.ts --production --outfile=dist/code.js --format=cjs --watch",
		"test": "bun test --coverage",
		"lint": "biome check --error-on-warnings",
		"lint:fix": "biome check --fix",
		"prepare": "husky"
	},
	"author": "",
	"license": "",
	"devDependencies": {
		"@figma/plugin-typings": "^1.121",
		"husky": "^9.1",
		"typescript": "^5.9",
		"@biomejs/biome": "^2.3",
		"@types/bun": "^1.3"
	},
	"dependencies": {
		"jszip": "^3.10"
	}
}
